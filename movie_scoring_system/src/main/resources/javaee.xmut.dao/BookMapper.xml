<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="javaee.xmut.dao.BookMapper">
    <select id="findBookById" parameterType="int" resultType="javaee.xmut.entity.Book">
        select * from book where id = #{id}
    </select>
</mapper>